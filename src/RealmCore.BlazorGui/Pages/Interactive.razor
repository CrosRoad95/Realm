@page "/realmUi/interactive"

Foo=@Gui.Foo
Hello @(Gui.Player.Name)

@code{
    [CascadingParameter]
    private InteractiveGui Gui { get; set; }

    protected override void OnAfterRender(bool firstRender)
    {
        if (firstRender)
        {
            Gui.FooChanged += () =>
            {
                InvokeAsync(StateHasChanged);
            };
        }
    }
}